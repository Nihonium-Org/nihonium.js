{
  "name": "nihonium.js",
  "type": "module",
  "version": "0.0.1",
  "description": "A library for interacting with multiple anime/manga/hentai website APIs. Including metadata, search, and more.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nihonium-Org/nihonium.js"
  },
  "homepage": "https://github.com/Nihonium-Org/nihonium.js",
  "bugs": {
    "url": "https://github.com/Nihonium-Org/nihonium.js/issues"
  },
  "keywords": [
    "anime",
    "manga",
    "hentai",
    "api",
    "library",
    "nodejs",
    "typescript"
  ],
  "author": "Nihonium Org.",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "format": "biome check --write",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe .",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "ky": "^1.8.1",
    "@types/jest": "^29.5.14",
    "axios": "^1.9.0",
    "jest": "^29.7.0"
  }
}